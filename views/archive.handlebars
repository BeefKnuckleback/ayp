{{#if prev}}
<div id="nav" class="archive top">
<a href="/archive/{{prev.time}}/" title="Prev Archive">Previous page of strips</a>
</div><!--#nav.archive top-->
{{/if}}

<div id="page">
{{#each archive}}
<div id="comic" class="archive">
	 <a href="{{ comicUrl this.time }}" title="{{ this.time }}">
	   <img src="{{ this.url }}" alt="{{ this.time }}" />
	 </a>
</div><!--#comic.archive-->
{{/each}}
</div><!--#page-->

{{#if next}}
<div id="nav" class="archive bottom">
<a href="/archive/{{next.time}}/" title="Next Page">Next page of strips</a>
</div><!--#nav.archive bottom-->
{{/if}}
